#header#

<TABLE border="0" cellspacing="0" cellpadding="0" width="500" height="100%">
    <tr>
        <td height="6px;">
            <table width="500" border="0" style='height:6px;' cellspacing="0">
                <tr>
                    <td background="#LTChatTemplatePath#img/chat_box/lt.bmp" style='width:4px;'></td>
                    <td bgcolor="White" style=""></td>
                    <td background="#LTChatTemplatePath#img/chat_box/rt.bmp" style='width:5px;'></td>
                </tr>
            </table>
        </td>
    </tr>
    <tr>
        <td bgcolor="White"
            style="vertical-align:text-top;padding-left:5px;padding-right:5px;border-right:1px #7A7B7B solid;">

            <table border="1" align="center" cellpadding="0" cellspacing="0" bordercolor="#CCCCCC"
                   style="margin-top:10px; border-collapse: collapse" width="500" height="100">

                <tr>
                    <td align="left" nowrap width="500" colspan="2" height="33" bgcolor="#E9E9E9">
                        <p align="center">#description#</td>
                </tr>

                <tr>
                    <td valign="top" align="left" nowrap height="25" width="500" bgcolor="#999999" colspan="2">
                        <p align="center"><font color="#FFFFFF">&nbsp;<span style="font-weight: 500"><font
                                            face="Verdana" style="font-size: 8pt">#you_level#</font></span></font></td>
                </tr>
                #items#
            </table>

        </td>
    </tr>
    <tr>
        <td height="6px;">
            <table width="100%" border="0" style='height:7px;' cellspacing="0">
                <tr>
                    <td background="#LTChatTemplatePath#img/chat_box/lb.bmp" style='width:5px;'></td>
                    <td bgcolor="White" style=""></td>
                    <td background="#LTChatTemplatePath#img/chat_box/rb.bmp" style='width:5px;'></td>
                </tr>
            </table>
        </td>
    </tr>
</TABLE>
#footer#